<!DOCTYPE html>
<html>
<head>
	<title>Belajar Leaflet.js</title>
	<link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css" />
	<script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js"></script>
	<style type="text/css">
		#map {
			position: absolute;
			top: 0;
			bottom: 0;
			left: 0;
			right: 0;

		}
	</style>
</head>
<body>
	<div id="map" style="width: 600px; height: 400px;"></div>
	<script type="text/javascript">
		var map = L.map('map').setView([-7.165977, 111.8767305], 17);
		L.tileLayer('https://api.maptiler.com/maps/streets/{z}/{x}/{y}.png?key=PpGwrAn6h8yqbm9RClUE', {
			attribution: '<a href="https://www.maptiler.com/copyright/" target="_blank">© MapTiler</a> <a href="https://www.openstreetmap.org/copyright" target="_blank">© OpenStreetMap contributors</a>',
		}).addTo(map);
		var marker = L.marker([-7.165977, 111.8767305]).addTo(map);
	</script>

</body>
</html>